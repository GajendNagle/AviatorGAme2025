            swal({ title: "Wallet Address", text: "Copied", type: "success" })
        });
        clipboard.on('error', function (e) {
            console.log(e);
        });
    </script>
    <script>
        $(document).ready(function () {
            var theButtons = $(".amount-btn");
            theButtons.each(function () {
                var currentButton = $(this);
                var buttonValue = currentButton.val();
                currentButton.click(function () {
                    $("#txtamtTRX").val(buttonValue);
                    $(".amount-btn").removeClass('active');
                    $(this).addClass('active');
                });
            });
        })
    </script>
    <script>
        $(document).ready(function () {
            $('#bankmode').click(function () {
                $('#cryptosection').hide();
                $('#upisection').hide();
                $('#banksection').show();
                $('#bankmode').addClass('active');
                $('#cryptomode').removeClass('active');
                $('#upimode').removeClass('active');
            })
            $('#cryptomode').click(function () {
                $('#banksection').hide();
                $('#upisection').hide();
                $('#cryptosection').show();
                $('#cryptomode').addClass('active');
                $('#bankmode').removeClass('active');
                $('#upimode').removeClass('active');
            })
            $('#upimode').click(function () {
                $('#banksection').hide();
                $('#upisection').show();
                $('#cryptosection').hide();
                $('#upimode').addClass('active');
                $('#cryptomode').removeClass('active');
                $('#bankmode').removeClass('active');
            })
        });
    </script>
